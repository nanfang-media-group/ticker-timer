{
    "name": "nanfang-media-group/ticker-timer",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^3.7|^3.8|^4.0|^5.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "NanfangMediaGroup\\TickerTimer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NanfangMediaGroup\\TickerTimer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NanfangMediaGroup\\TickerTimer\\TickerTimerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
